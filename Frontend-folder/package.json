{
  "name": "vehicle-system",
  "version": "0.0.1",
  "author": "Nikhil <nikhil.narayana@presight.ai>",
  "engines": {
    "node": "^14.20.0 || ^16.13.0 || >=18.16.0",
    "yarn": "Please use NPM instead of yarn to install dependencies",
    "npm": ">=8.19.2 <=9.6.7"
  },
  "scripts": {
    "ng": "ng",
    "start": "npm run lint && npm run stylelint:fix && ng serve --proxy-config proxy.conf.json",
    "start:hmr": "ng serve --hmr --open",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "eslint": "eslint src/**/*.{js,ts} --max-warnings=0",
    "eslint:fix": "eslint src/**/*.{js,ts} --max-warnings=0 --fix",
    "stylelint": "stylelint **/*.scss",
    "stylelint:fix": "stylelint src/**/*.scss --fix",
    "test": "ng test",
    "build": "ng build",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "commit": "czg",
    "release": "HUSKY=0 SKIP=prepare-commit-msg standard-version --no-verify"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~16.0.0",
    "@angular/common": "~16.0.0",
    "@angular/compiler": "~16.0.0",
    "@angular/core": "~16.0.0",
    "@angular/forms": "~16.0.0",
    "@angular/platform-browser": "~16.0.0",
    "@angular/platform-browser-dynamic": "~16.0.0",
    "@angular/router": "~16.0.0",
    "@angular/service-worker": "~16.0.0",
    "@ng-select/ng-select": "~11.0.0",
    "@ngneat/transloco": "~4.2.2",
    "@stomp/stompjs": "6.1.2",
    "sockjs-client": "1.6.1",
    "ag-grid-angular": "~30.0.2",
    "ag-grid-community": "~30.0.2",
    "echarts": "~5.4.3",
    "ngx-daterangepicker-material": "~6.0.4",
    "ngx-pagination": "~6.0.3",
    "ngx-toastr": "~17.0.2",
    "ngx-ui-loader": "~13.0.0",
    "rxjs": "~7.8.0",
    "tslib": "~2.3.0",
    "zone.js": "~0.13.0",
    "glob": "~10.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~16.0.2",
    "@angular-eslint/builder": "~16.0.2",
    "@angular-eslint/eslint-plugin": "16.0.2",
    "@angular-eslint/eslint-plugin-template": "16.0.2",
    "@angular-eslint/schematics": "16.0.2",
    "@angular-eslint/template-parser": "16.0.2",
    "@angular/cli": "~16.0.2",
    "@angular/compiler-cli": "~16.0.0",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@types/jasmine": "~4.3.0",
    "@types/sockjs-client": "^1.5.4",
    "@types/video.js": "^7.3.53",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "autoprefixer": "^10.4.14",
    "czg": "^1.6.1",
    "eslint": "^8.40.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": "^13.2.2",
    "ngx-echarts": "16.0.0",
    "postcss": "^8.4.24",
